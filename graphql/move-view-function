query {
  iotaNamesAuction: moveViewCall(
    functionName: "0x5e7a300e640f645a4030aeb507c7be16909e6fa9711e7ca2d4397bbd967d5c50::auction::get_auction_metadata"
    arguments: [
      "0x31deb8cbd320867089d52c37fed2d443520aac0fc5a957de1f64f9135b83f42b"
      "auc.iota"
    ]
  ) {
    error
    results
  }
}

curl -X POST https://graphql.devnet.iota.cafe \
     --header 'x-iota-rpc-show-usage: true' \
     --header 'Content-Type: application/json' \
     --data '{
        "query": "query { iotaNamesAuction: moveViewCall( functionName: \"0x5e7a300e640f645a4030aeb507c7be16909e6fa9711e7ca2d4397bbd967d5c50::auction::get_auction_metadata\" arguments: [ \"0x31deb8cbd320867089d52c37fed2d443520aac0fc5a957de1f64f9135b83f42b\" \"auc.iota\" ] ) { error results } }"
      }' | jq .
